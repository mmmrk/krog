= form_for(@clientele) do |f|
	- if @clientele.errors.any?
		#error_explanation
			%h2 #{ pluralize(@clientele.errors.count, "error") } prohibited this clientele from being saved:
			%ul 
				- @clientele.errors.full_messages.each do |msg|
					%li= msg
		
	.field
		= f.label :name
		%br
		= f.text_field :name

	.actions
		= f.submit