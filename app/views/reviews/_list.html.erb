<table>
	<tbody>
		<% @reviews.each do |review| %>
			<tr>
				<td><%= review.rating.value %></td>
				<td><%= review.visit_date %></td>
				<td><%= review.comment %></td>
				<td><%= review.establishment.name %></td>
				<td><%= review.category.name %></td>
				<td><%= review.clientele.name %></td>
				<td><%= review.sound_level.name %></td>
				<td><%= review.hygiene.name %></td>
				<td><%= link_to 'Show', review %></td>
				<td><%= link_to 'Edit', edit_review_path(review) %></td>
				<td><%= link_to 'Destroy', review, :confirm => 'Are you sure?', :method => :delete %></td>
			</tr>
		<% end %>
	</tbody>
</table>